{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
      
    ]
  },
  "rules": { 
    "no-empty-interface": false,
    "member-access" : false,
    "variable-name": false,
    "no-console": false,
    "object-literal-sort-keys": false
  }
}
